<p id="notice"><%= notice %></p>

<h1>Utf Items</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @utf_items.each do |utf_item| %>
      <tr>
        <td><%= utf_item.name %></td>
        <td><%= utf_item.description %></td>
        <td><%= link_to 'Show', utf_item %></td>
        <td><%= link_to 'Edit', edit_utf_item_path(utf_item) %></td>
        <td><%= link_to 'Destroy', utf_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Utf Item', new_utf_item_path %>
